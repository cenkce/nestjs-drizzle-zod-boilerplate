{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "experimentalDecorators": true,
    "module": "NodeNext",
    "strict": true,
    "noImplicitAny": false,
    "declaration": true,
    "outDir": "./dist",
    "removeComments": true,
    "sourceMap": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "dist",
  ]
}